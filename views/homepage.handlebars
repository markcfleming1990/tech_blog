<article class="post">

    {{#if loggedIn}}
    <button type="submit" class="button-margin"><a class="new-post-button " href="/dashboard/new">Add New Post</a></button>
    {{/if}}

    {{#each posts as |post| }}
        {{> post-info post}}
    {{/each}}


</article>


<script src="/javascript/add-post.js"></script>